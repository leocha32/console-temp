{
  "extends": "../../tsconfig.json",
  "typeRoot": ["src/assets/images"],
  "compilerOptions": {
    "outDir": "dist",
    "baseUrl": "src",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": ["jest", "lodash", "gapi", "gapi.auth2"]
  },
  "include": ["src"]
}
